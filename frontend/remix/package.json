{
    "name": "render",
    "version": "0.1.8",
    "license": "MIT",
    "type": "module",
    "sideEffects": false,
    "exports": {
        ".": {
            "types": "./build.d.ts",
            "default": "./build/index.js"
        },
        "./package.json": "./package.json"
    },
    "scripts": {
        "build": "concurrently \"cross-env NODE_ENV=production remix build\" \"yarn build:worker\" ",
        "build:worker": "esbuild ./app/entry.worker.tsx --outfile=./public/entry.worker.js --minify --bundle --format=esm ",
        "watch": "concurrently  \"cross-env NODE_ENV=development remix watch\" \"yarn watch:worker\"",
        "watch:worker": "nodemon --exec \"yarn build:worker\" --watch ./app/entry.worker.tsx",
        "lint": "eslint .",
        "typecheck": "tsc -b"
    },
    "dependencies": {
        "@picocss/pico": "^1.5.0",
        "@remix-run/express": "^1.7.0",
        "@remix-run/react": "^1.7.0",
        "@remix-run/server-runtime": "^1.7.0",
        "@remix-validated-form/with-zod": "^2.0.1",
        "@web3-storage/multipart-parser": "^1.0.0",
        "ariakit": "^2.0.0-next.40",
        "chainlocker": "^1.3.2",
        "clsx": "^1.2.1",
        "compression": "^1.7.4",
        "express": "^4.18.1",
        "gun": "^0.2020.1239",
        "isbot": "^3.4.5",
        "jsesc": "^3.0.2",
        "lz-string": "^1.4.4",
        "morgan": "^1.10.0",
        "prismjs": "^1.27.0",
        "react": "^18.2.0",
        "react-dom": "^18.2.0",
        "redaxios": "^0.5.1",
        "remix": "^1.7.0",
        "remix-validated-form": "^4.5.5",
        "stellar-sdk": "^10.4.1",
        "stripe": "^11.5.0",
        "super-expressive": "^1.4.1",
        "zod": "^3.18.0",
        "zod-form-data": "^1.2.1"
    },
    "devDependencies": {
        "@remix-run/dev": "^1.7.0",
        "@remix-run/eslint-config": "^1.7.0",
        "@remix-run/node": "^1.7.0",
        "@remix-run/serve": "^1.7.0",
        "@types/jsesc": "^3.0.1",
        "@types/lz-string": "^1.3.34",
        "@types/prismjs": "^1.26.0",
        "@types/react": "^18.0.14",
        "@types/react-dom": "^18.0.5",
        "@types/ws": "^8.5.4",
        "concurrently": "^7.3.0",
        "cross-env": "^7.0.3",
        "eslint": "^8.15.0",
        "eslint-config-custom": "*",
        "nodemon": "^2.0.15",
        "npm-run-all": "^4.1.5",
        "rollup-plugin-node-polyfills": "^0.2.1",
        "tsconfig": "*",
        "typescript": "^4.6.4"
    }
}
