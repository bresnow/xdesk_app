{
    "name": "server",
    "version": "0.1.8",
    "license": "MIT",
    "type": "module",
    "scripts": {
        "watch": "cross-env NODE_ENV=development  PORT=3333 SECRET_KEY=\"SecretKeyToken\"  nodemon ./index.js --watch ../ui/**/* ../chain-extensions/* ./index.js",
        "start": "cross-env-shell NODE_ENV=production PORT=$PORT SECRET_KEY=$SECRET_KEY node ./index.js",
        "lint": "eslint .",
        "typecheck": "tsc -b",
        "format": "prettier --write '**/*.{json,md,mdx,mjs,mts,js,jsx,ts,tsx}'"
    },
    "dependencies": {
        "chainlocker": "^1.3.2",
        "render": "*"
    },
    "devDependencies": {
        "render": "*"
    }
}
